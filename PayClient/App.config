<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!--支付代理服务器的ip或域名和端口-->
    <add key="PayProxy" value="localhost:8080"/>

    <!--将Http请求参数转发到此LocalURL，不包含路径-->
    <!--此URL一般为IIS等本机服务器提供，使用VS等调试工具将源码附加到IIS进程，就可以下断查看http请求参数-->
    <add key="LocalURL" value="http://localhost"/>
  </appSettings>
</configuration>